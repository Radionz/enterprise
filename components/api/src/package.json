{
  "name": "stoplight-core",
  "version": "1.0.2",
  "description": "The modular API toolchain and platform.",
  "author": "Marc MacLeod <marc@stoplight.io>",
  "engines": {
    "node": "8.1.x"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/stoplightio/core.git"
  },
  "scripts": {
    "api:dev": "NODE_ENV=development NODE_CONFIG_DIR=config/api nodemon internals/run/start.api.js -e js",
    "api:debug": "DEBUG=* npm run api:dev",
    "api:build": "npm run api:build:pre && NODE_ENV=production webpack --config internals/webpack/api/webpack.prod.babel.js --color -p && npm run api:build:post",
    "api:build:pre": "npm run test:clean && rm -rf ./build/api/production",
    "api:build:post": "cp -R ./src/api/public ./build/api/production/public && cp ./Procfile ./build/api/production && cp ./package.json ./build/api/production && cp ./package-lock.json ./build/api/production && cp internals/docker/api.dockerfile ./build/api/production && touch ./build/api/production/.gitignore",
    "api:deploy:production": "npm run api:build && cd build/api/production && heroku builds:create -a stoplight-api-next",
    "api:build:staging": "npm run api:build:staging:pre && NODE_ENV=production BUILD_ENV=staging webpack --config internals/webpack/api/webpack.staging.babel.js --color -p && npm run api:build:staging:post",
    "api:build:staging:pre": "npm run test:clean && rm -rf ./build/api/staging",
    "api:build:staging:post": "cp -R ./src/api/public ./build/api/staging/public && cp ./Procfile ./build/api/staging && cp ./package.json ./build/api/staging && touch ./build/api/staging/.gitignore",
    "api:deploy:staging": "npm run api:build:staging && cd build/api/staging && heroku builds:create -a stoplight-api-next-staging",
    "api:dockerize": "ENTERPRISE=1 npm run api:build && cd build/api/production && docker build -f api.dockerfile -t stoplight/api .",
    "api:dockerize:push": "npm run api:dockerize && ./internals/docker/pushApi.dev.sh",
    "api:test": "mocha src/api/test/ --recursive",
    "app:dev": "NODE_ENV=development webpack-dashboard -m -p 3499 -- node ./internals/server/app",
    "app:build": "npm run app:build:pre && NODE_ENV=production webpack --config internals/webpack/app/webpack.prod.babel.js --color -p && npm run app:build:post",
    "app:build:pre": "npm run test:clean && rm -rf ./build/app/production",
    "app:build:post": "cp internals/docker/app.dockerfile ./build/app/production",
    "app:build:staging": "npm run app:build:staging:pre && NODE_ENV=production BUILD_ENV=staging webpack --config internals/webpack/app/webpack.staging.babel.js --color -p",
    "app:build:staging:pre": "npm run test:clean && rm -rf ./build/app/staging",
    "app:deploy:production": "npm run app:build && npm run app:deploy:pre && netlify deploy -s 1860c3d7-389d-403d-82b2-8c1246842f49 -p build/app/production",
    "app:deploy:pre": "cp internals/deploy/app/netlify/* build/app/production",
    "app:deploy:staging": "npm run app:build:staging && npm run app:deploy:staging:pre && netlify deploy -s 81a6f0a8-d4ab-4381-bfb2-8039ae2c1a5f -p build/app/staging",
    "app:deploy:staging:pre": "cp internals/deploy/app/netlify/* build/app/staging",
    "app:dockerize": "ENTERPRISE=1 npm run app:build && cd build/app/production && docker build -f app.dockerfile -t stoplight/app .",
    "app:dockerize:push": "npm run app:dockerize && ./internals/docker/pushApp.dev.sh",
    "app:test": "./node_modules/.bin/mocha --require ./internals/testing/setup.js './src/**/*/__tests__/*.js' --bail --reporter=list --timeout=10000",
    "app:test:watch": "npm run app:test -- --watch",
    "exporter:dev": "NODE_ENV=development NODE_CONFIG_DIR=config/exporter nodemon internals/run/start.exporter.js -e js",
    "exporter:debug": "DEBUG=* npm run exporter:dev",
    "exporter:build": "npm run exporter:build:pre && NODE_ENV=production webpack --config internals/webpack/exporter/webpack.prod.babel.js --color -p && npm run exporter:build:post",
    "exporter:build:pre": "npm run test:clean && rm -rf ./build/exporter/production",
    "exporter:build:post": "cp -R ./src/exporter/public ./build/exporter/production/public && cp ./Procfile ./build/exporter/production && cp ./package.json ./build/exporter/production && cp ./package-lock.json ./build/exporter/production && cp internals/docker/exporter.dockerfile ./build/exporter/production && touch ./build/exporter/production/.gitignore",
    "exporter:deploy:production": "npm run exporter:build && cd build/exporter/production && heroku builds:create -a stoplight-export",
    "exporter:build:staging": "npm run exporter:build:staging:pre && NODE_ENV=production BUILD_ENV=staging webpack --config internals/webpack/exporter/webpack.staging.babel.js --color -p && npm run exporter:build:staging:post",
    "exporter:build:staging:pre": "npm run test:clean && rm -rf ./build/exporter/staging",
    "exporter:build:staging:post": "cp -R ./src/exporter/public ./build/exporter/staging/public && cp ./Procfile ./build/exporter/staging && cp ./package.json ./build/exporter/staging && touch ./build/exporter/staging/.gitignore",
    "exporter:deploy:staging": "npm run exporter:build:staging && cd build/exporter/staging && heroku builds:create -a stoplight-export-staging",
    "exporter:dockerize": "ENTERPRISE=1 npm run exporter:build && cd build/exporter/production && docker build -f exporter.dockerfile -t stoplight/exporter .",
    "exporter:dockerize:push": "npm run exporter:dockerize && ./internals/docker/pushExporter.dev.sh",
    "exporter:test": "mocha src/exporter/test/ --recursive",
    "lint": "eslint . --ext .jsx,.js --ignore-path .gitignore",
    "build:clean": "npm run test:clean && rm -rf ./build",
    "build:dll": "node ./internals/run/build.dll.js",
    "test:clean": "rm -rf ./coverage",
    "migrate:dev": "NODE_ENV=development NODE_CONFIG_DIR=config/migration node ./migrations",
    "migrate:staging": "NODE_ENV=staging NODE_CONFIG_DIR=config/migration node ./migrations",
    "migrate:prod": "NODE_ENV=production NODE_CONFIG_DIR=config/migration node ./migrations",
    "format": "prettier --single-quote --trailing-comma es5 --print-width 100 --write \"src/**/*.js\"",
    "setup": "rm package-lock.json || true && rm -rf ./node_modules && npm i && npm run build:dll"
  },
